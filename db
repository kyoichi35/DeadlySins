while wait(5) do
    if getgenv().findballs then
    if not game.Players.LocalPlayer.Name then return end
        for index, balls in pairs(workspace:GetDescendants()) do
            if balls:IsA("Part") and balls.Name == "Collision" then
                game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = balls.CFrame
                wait(1.3)
                if game:GetService("ReplicatedStorage").Events.Update and (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - balls.Position).magnitude < 5 then
                local Event = game:GetService("ReplicatedStorage").Events.Update
                Event:FireServer()
    if getgenv().ServerHop then
    local Space = 4195907096
    local Namek = 4195906474
    local Earth = 4638110048
    
    if game.PlaceId == Earth then
    
    
        local Event = game:GetService("ReplicatedStorage").Events.EnforceSave
        Event:InvokeServer()
        
         
        local A_1 = Namek
        local Event = game:GetService("ReplicatedStorage").VIPServerCreator
        Event:InvokeServer(A_1)
        end
        
    
    if game.PlaceId == Namek then
    
    
        local Event = game:GetService("ReplicatedStorage").Events.EnforceSave
        Event:InvokeServer()
        
         
        local A_1 = Earth
        local Event = game:GetService("ReplicatedStorage").VIPServerCreator
        Event:InvokeServer(A_1)
        end
    
    
    end
                wait(2.5)
                Event:Destroy()
                
    end
            end
        end
    end
    end
