local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Kyoichi Hub", "BloodTheme")
    -- MAIN
    
    local Other = Window:NewTab("Main")
    local OtherSection = Other:NewSection("Auto Spin")

    OtherSection:NewButton("Auto Spin Race", "Auto Spins your Race till legendary", function()
        loadstring(game:HttpGet(("https://raw.githubusercontent.com/kyoichi35/DeadlySins/main/AutoSpinRace"),true))()
    end)

    OtherSection:NewButton("Auto Spin Magic", "Auto Spins your Magic till legendary", function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/kyoichi35/DeadlySins/main/AutoSpinMagic"))() 
    end)
